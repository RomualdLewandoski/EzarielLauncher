    <div class="optionCard">
        <div class="optionHead row p-0 m-0" id="optionRAM">
            <div class="col-1 my-auto">
                <i class="far fa-bug fa-2x"></i>
            </div>
            <div class="col-10 my-auto">
                <span class="font-weight-bold">Debug</span><br>
                <span>Options diverses</span>
            </div>
            <div class="col-1 text-right my-auto">
                <i class="far fa-angle-down fa-2x"></i>
            </div>
        </div>
        <div class="optionContent">
            <div class="row">
                <div class="col-4">
                    <div class="btn btn-warning" id="openConsole"><i class="far fa-terminal"></i> Ouvrir la console</div>
                </div>
                <div class="col-4">
                    <div class="btn btn-danger" id="deleteData"><i class="far fa-bomb"></i> Supprimer les données du
                        launcher
                    </div>
                </div>

                <div class="col-4">
                    <select name="" id="themePicker" class="form-control">
                        <option value="">Thème</option>
                        <option value="default">Defaut</option>
                        <option value="modern">Moderne</option>
                    </select>
                </div>

            </div>

        </div>
    </div>


    <div id="" style="display: none;">
